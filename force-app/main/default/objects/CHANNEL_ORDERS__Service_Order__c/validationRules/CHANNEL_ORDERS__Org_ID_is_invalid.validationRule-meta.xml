<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CHANNEL_ORDERS__Org_ID_is_invalid</fullName>
    <active>true</active>
    <errorConditionFormula>OR(not(begins( CHANNEL_ORDERS__Customer_Org_ID__c ,&quot;00D&quot;)),AND( LEN(CHANNEL_ORDERS__Customer_Org_ID__c) &lt;&gt;15))</errorConditionFormula>
    <errorDisplayField>CHANNEL_ORDERS__Customer_Org_ID__c</errorDisplayField>
    <errorMessage>The Org ID you entered does not follow the valid format.  Org IDs should begin with &quot;00D&quot; and be 15 characters in length.</errorMessage>
</ValidationRule>
